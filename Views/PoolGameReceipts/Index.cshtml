@model IEnumerable<TMP2019.Controllers.PoolGamesControllers.DataModels.PoolGameReceipt>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Arena)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1Fee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1TravelKost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1Alowens)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1LateGameKost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1Other)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HD1TotalFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Signature)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Arena.ArenaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1.CName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1Fee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1TravelKost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1Alowens)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1LateGameKost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1Other)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HD1TotalFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Signature)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
